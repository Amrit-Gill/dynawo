<dyn:dynamicModelsArchitecture xmlns:dyn="http://www.rte-france.com/dynawo">
  <!-- Synchronous generator with four windings, ENSTOE AVR, Governor but no PSS-->
  <dyn:modelicaModel id="GeneratorSynchronousFourWindingsENSTOERegulationsWithoutPSS">
    <dyn:unitDynamicModel id="generator" name="Dynawo.Electrical.Machines.GeneratorSynchronous" initName = "Dynawo.Electrical.Machines.GeneratorSynchronousExt_4E_INIT"/>
    <dyn:unitDynamicModel id="RefLPu" name="Dynawo.Electrical.Controls.Basics.SetPoint" initName="Dynawo.Electrical.Controls.Basics.SetPoint_INIT"/>
    <dyn:unitDynamicModel id="VrefPu" name="Dynawo.Electrical.Controls.Basics.Step" initName="Dynawo.Electrical.Controls.Basics.Step_INIT"/>
    <dyn:unitDynamicModel id="omegaRefPu" name="Dynawo.Electrical.Controls.Basics.SetPoint"/>
    <dyn:unitDynamicModel id="VsPu" name="Dynawo.Electrical.Controls.Basics.SetPoint"/>
    <dyn:unitDynamicModel id="voltageRegulator" name="Dynawo.Electrical.Controls.Machines.VoltageRegulators.IEEE_AC4A" initName="Dynawo.Electrical.Controls.Machines.VoltageRegulators.IEEE_AC4A_INIT_WithoutPSS"/>
    <dyn:unitDynamicModel id="governor" name="Dynawo.Electrical.Controls.Machines.Governors.IEEE_TGOV1" initName="Dynawo.Electrical.Controls.Machines.Governors.IEEE_TGOV1_INIT"/>
    <dyn:initConnect id1="voltageRegulator" var1="efd0Pu" id2="generator" var2="Efd0Pu"/>
    <dyn:initConnect id1="voltageRegulator" var1="Vc0Pu" id2="generator" var2="UStator0Pu"/>
    <dyn:initConnect id1="voltageRegulator" var1="Vref0Pu" id2="VrefPu" var2="Value0"/>
    <dyn:initConnect id1="governor" var1="PMech0Pu" id2="generator" var2="Pm0Pu"/>
    <dyn:initConnect id1="governor" var1="RefL0Pu" id2="RefLPu" var2="Value0"/>
    <dyn:connect id1="omegaRefPu" var1="setPoint" id2="generator" var2="omegaRefPu"/>
    <dyn:connect id1="voltageRegulator" var1="EfdPuPin" id2="generator" var2="efdPu"/>
    <dyn:connect id1="voltageRegulator" var1="VsPu" id2="VsPu" var2="setPoint.value"/>
    <dyn:connect id1="voltageRegulator" var1="VrefPu" id2="VrefPu" var2="step.value"/>
    <dyn:connect id1="voltageRegulator" var1="VcPu" id2="generator" var2="UStatorPu.value"/>
    <dyn:connect id1="governor" var1="RefLPu" id2="RefLPu" var2="setPoint.value"/>
    <dyn:connect id1="governor" var1="omegaPu" id2="generator" var2="omegaPu.value"/>
    <dyn:connect id1="governor" var1="PmPu" id2="generator" var2="PmPu"/>
  </dyn:modelicaModel>
</dyn:dynamicModelsArchitecture>
